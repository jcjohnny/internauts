
<%= form_for(@article) do |f| %>
    <ul>
        <% @article.errors.full_messages.each do |error| %>
        <li><%= error %></li>
        <% end %>
    </ul>
    <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
    </p>
    <p>
    <%= f.label :body %><br />
    <%= f.text_area :body %>
    </p>

    <p>
    <%= f.label :rating %>
    <%= f.number_field :rating %>
    </p>
    <p>
        <%= f.label :address %>
        <%= f.text_field :address %>
    </p>
    <p>
        <%= f.label :city %>
        <%= f.text_field :city %>
    </p>
    <p>
        <%= f.label :state %>
        <%= f.text_field :state %>
    </p>
    <p>
        <%= f.label :zipcode %>
        <%= f.number_field :zipcode %>
    </p>
    <p>
        <%= f.label :sport %>
        <%= f.text_field :sport %>
    </p>
    <p>
        <%= f.label :players %>
        <%= f.number_field :players %>
    </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
